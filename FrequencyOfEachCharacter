import java.util.*;
import java.util.stream.*;
import java.util.Map.*;
import java.util.function.Function;
class HelloWorld {
    public static void main(String[] args) {
        String a="Deepika";
        Arrays.stream(a.split("")).collect(Collectors.groupingBy(Function.identity(),Collectors.counting())).entrySet().stream().forEach(System.out::println);
        System.out.println("Try programiz.pro");
    }
}
